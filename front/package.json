{
  "name": "test",
  "version": "1.0.0",
  "description": "essai react",
  "scripts": {
    "clean": "rimraf public",
    "build:webpack": "set NODE_ENV=production && webpack --progress --colors --config ./webpack/webpack.config.prod.js",
    "build:webpack_unix": "export NODE_ENV=production && webpack --optimize-minimize --progress --colors --config ./webpack/webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node ./webpack/devServer.js && node node_modules/open/lib/open.js src/app/index.html",
    "start-dev": "nodemon ./webpack/devServer.js && node node_modules/open/lib/open.js src/app/index.html",
    "lint": "eslint src/js",
    "test": "set NODE_ENV=test && karma start src/test/config/karma.conf.js"
  },
  "keywords": [
    "react",
    "reactjs",
    "maboxrh",
    "hot",
    "reload",
    "hmr",
    "live",
